# Masumi Node Configuration
# Copy to .env.masumi and fill in values

# Admin key for dashboard access
ADMIN_KEY=change_me_to_a_secure_random_string

# Encryption key for wallet data
ENCRYPTION_KEY=change_me_to_another_secure_random_string

# Blockfrost API key for Cardano Preprod
# Get one at https://blockfrost.io
BLOCKFROST_API_KEY_PREPROD=preprodXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# Network
NETWORK=PREPROD
